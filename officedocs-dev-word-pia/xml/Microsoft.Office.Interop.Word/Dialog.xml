    <Member MemberName="Show">
      <MemberSignature Language="C#" Value="public int Show (ref object TimeOut);" />
      <MemberSignature Language="ILAsm" Value=".method public hidebysig newslot virtual instance int32 Show([in]object&amp; TimeOut) runtime managed" />
