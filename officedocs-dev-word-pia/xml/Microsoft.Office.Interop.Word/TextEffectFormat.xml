                </item>
                <item>
                  <description>Loose</description>
                  <description>1.2</description>
                </item>
                <item>
                  <description>Very Loose</description>
                  <description>1.5</description>
                </item>
              </list>
              <para />
            </span>
          </span>
        </remarks>
